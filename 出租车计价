#include<stdio.h>
int main(){
	double s,m;
	int t;
	scanf("%lf %d",&s,&t);
	if(t>=5){
	if(s<=3) m=10+t/5*2;
	if(s>3&&s<=10) m=(s-3)*2+10+t/5*2;
	if(s>10) m=(s-10)*3+24+t/5*2;
	printf("%.0lf",m);}
	if(t<5){
	if(s<=3) m=10;
	if(s>3&&s<=10) m=(s-3)*2+10;
	if(s>10) m=(s-10)*3+24;
	printf("%.0lf",m);
	}
}
